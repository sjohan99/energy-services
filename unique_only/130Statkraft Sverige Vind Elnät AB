L\u00e4s mer om Vindpark \u00d6gonf\u00e4gnaden<\/a><\/span>[\/et_pb_map_pin][\/et_pb_map][\/et_pb_column][\/et_pb_row][et_pb_row padding_mobile=”off” column_padding_mobile=”on” custom_padding_last_edited=”on|phone” disabled_on=”||on” _builder_version=”3.25″ background_size=”initial” background_position=”top_left” background_repeat=”repeat” custom_margin_tablet=”-20px|||” custom_margin_phone=”-40px|||” custom_margin_last_edited=”on|phone” custom_padding_tablet=”” custom_padding_phone=”0px|||” global_colors_info=”{}”][et_pb_column type=”4_4″ _builder_version=”3.25″ custom_padding=”|||” custom_padding_phone=”0px|||” custom_padding_last_edited=”on|phone” global_colors_info=”{}” custom_padding__hover=”|||” custom_padding_tablet=”|||”][et_pb_map address=”Sverige” zoom_level=”7″ address_lat=”63.399782688357504″ address_lng=”16.548202232910153″ mouse_wheel=”off” use_grayscale_filter=”on” grayscale_filter_amount=”90″ _builder_version=”3.0.91″ child_filter_saturate=”10%” global_colors_info=”{}”][et_pb_map_pin title=”Bj\u00f6rkh\u00f6jden” pin_address=”969, 880 40 Ramsele, Sverige” pin_address_lat=”63.4256304020719″ pin_address_lng=”16.15894618650816″ _builder_version=”3.0.47″ global_colors_info=”{}”] [\/et_pb_map_pin][et_pb_map_pin title=”Stam\u00e5sen” pin_address=”Unnamed Road, 833 93 Str\u00f6msund, Sverige” pin_address_lat=”63.664942″ pin_address_lng=”15.820312999999942″ _builder_version=”3.0.47″ global_colors_info=”{}”] [\/et_pb_map_pin][et_pb_map_pin title=”M\u00f6rttj\u00e4rnberget” pin_address=”NYBODARNA-ALAN\u00c4SET 107, 840 67 S\u00f6rbygden, Sverige” pin_address_lat=”62.738257″ pin_address_lng=”15.947304179237335″ _builder_version=”3.0.47″ global_colors_info=”{}”] [\/et_pb_map_pin][et_pb_map_pin title=”\u00d6gonf\u00e4gnaden” pin_address=”Unnamed Road, 830 70 Hammerdal, Sverige” pin_address_lat=”63.52827983037531″ pin_address_lng=”15.953741803566004″ _builder_version=”3.0.47″ global_colors_info=”{}”] [\/et_pb_map_pin][\/et_pb_map][\/et_pb_column][\/et_pb_row][\/e