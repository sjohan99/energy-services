[et_pb_section admin_label=”Sektion” fullwidth=”on” specialty=”off” transparent_background=”off” background_color=”#ffffff” allow_player_pause=”off” inner_shadow=”off” parallax=”off” parallax_method=”off” padding_mobile=”off” make_fullwidth=”off” use_custom_width=”off” width_unit=”on” make_equal=”off” use_custom_gutter=”off”][et_pb_fullwidth_header admin_label=”Sidhuvud – helbredd” title=”Driftinformation” background_layout=”dark” text_orientation=”left” header_fullscreen=”on” header_scroll_down=”off” background_url=”http:\/\/statkraftvind.se\/wp-content\/uploads\/2016\/09\/driftinformation.jpg” background_overlay_color=”rgba(25,25,25,0.54)” parallax=”on” parallax_method=”off” content_orientation=”bottom” image_orientation=”bottom” custom_button_one=”on” button_one_letter_spacing=”0″ button_one_use_icon=”off” button_one_icon_placement=”right” button_one_on_hover=”on” button_one_letter_spacing_hover=”0″ custom_button_two=”off” button_two_letter_spacing=”0″ button_two_use_icon=”default” button_two_icon_placement=”right” button_two_on_hover=”on” button_two_letter_spacing_hover=”0″ title_font=”Open Sans Light||||” title_font_size=”53px” max_width=”45%” subhead_font_size=”19px” subhead_font_size_last_edited=”on|phone” custom_css_subtitle=”line-height: 24px; ” scroll_down_icon=”%%1%%” title_font_size_last_edited=”on|phone” max_width_last_edited=”on|desktop” subhead_font_size_phone=”18px” max_width_phone=”90%” title_font_size_phone=”33px” button_one_border_radius=”0″ button_two_border_radius=”0″ button_one_border_radius_hover=”0″ button_two_border_radius_hover=”0″ subhead=”H\u00e4r hittar du driftinformation f\u00f6r v\u00e5ra fyra vindparker. H\u00e5ll dig alltid uppdaterad med det senaste genom att prenumerera p\u00e5 v\u00e5r driftinformation. V\u00e4lj om du vill ha den via sms eller e-post.” button_one_url=”\/aktuellt-2\/driftinformation\/#footer-widgets” content_font_size=”18px” button_one_text=”Prenumerera p\u00e5 driftinformation”] [\/et_pb_fullwidth_header][\/et_p