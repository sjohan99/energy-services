[\/et_pb_text][\/et_pb_column][et_pb_column type=”1_3″ _builder_version=”4.16″ _module_preset=”default” background_color=”#FFFFFF” custom_padding=”||1.5em||false|false” border_radii=”on|2rem|2rem|2rem|2rem” global_colors_info=”{}”][et_pb_blurb title=”Tydlig milj\u00f6profil” image=”https:\/\/www.nordicgreen.fi\/wp-content\/uploads\/sites\/2\/nordic_green_energy_icons_rgb_kerrostalo_green.png” _builder_version=”4.16″ _module_preset=”6326d345-774d-4548-b917-430a07c4185c” global_colors_info=”{}”][\/et_pb_blurb][et_pb_text _builder_version=”4.16″ _module_preset=”45bfb6dc-4820-4457-8199-4737aad8905f” global_colors_info=”{}”]<\/p>\n
