[\/et_pb_text][\/et_pb_column][\/et_pb_row][et_pb_row column_structure=”1_4,1_4,1_4,1_4″ use_custom_gutter=”on” make_equal=”on” _builder_version=”4.16″ _module_preset=”default” max_width=”100%” custom_padding=”||72px||false|false” border_radii=”off|2rem|2rem||” global_colors_info=”{}”][et_pb_column type=”1_4″ _builder_version=”4.16″ _module_preset=”default” background_color=”#FFFFFF” custom_padding=”||1.5em||false|false” link_option_url=”\/teckna-elavtal\/” border_radii=”on|2rem|2rem|2rem|2rem” global_colors_info=”{}”][et_pb_blurb title=”Jag vill bli kund” image=”https:\/\/www.nordicgreen.fi\/wp-content\/uploads\/sites\/2\/nordic_green_energy_icons_rgb_aurinkovoima_green.png” _builder_version=”4.16″ _module_preset=”6326d345-774d-4548-b917-430a07c4185c” global_colors_info=”{}”][\/et_pb_blurb][et_pb_text _builder_version=”4.18.0″ _module_preset=”45bfb6dc-4820-4457-8199-4737aad8905f” global_colors_info=”{}”]<\/p>\n
